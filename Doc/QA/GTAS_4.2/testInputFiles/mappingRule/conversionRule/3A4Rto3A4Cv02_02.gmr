              <xsl:value-of select="/Pip3A4PurchaseOrderRequest/thisDocumentGenerationDateTime/DateTimeStamp"/>
            </DateTimeStamp>
          </requestingDocumentDateTime>
        </xsl:template>
        <xsl:template name="Pip3A4PurchaseOrderConfirmation-requestingDocumentIdentifier">
          <requestingDocumentIdentifier>
            <ProprietaryDocumentIdentifier>
              <xsl:value-of select="/Pip3A4PurchaseOrderRequest/thisDocumentIdentifier/ProprietaryDocumentIdentifier"/>
            </ProprietaryDocumentIdentifier>
          </requestingDocumentIdentifier>
        </xsl:template>
        <xsl:template name="Pip3A4PurchaseOrderConfirmation-thisDocumentGenerationDateTime">
          <thisDocumentGenerationDateTime>
            <xsl:variable name="varNow">
              <xsl:call-template name="Now">
                <xsl:with-param name="format" select="&apos;yyyyMMdd&apos;"/>
              </xsl:call-template>
            </xsl:variable>
            <xsl:variable name="varNow_1">
              <xsl:call-template name="Now">
                <xsl:with-param name="format" select="&apos;HHmmss.SSS&apos;"/>
              </xsl:call-template>
            </xsl:variable>
            <DateTimeStamp>
              <xsl:value-of select="concat($varNow, &apos;T&apos;, $varNow_1, &apos;Z&apos;)"/>
            </DateTimeStamp>
          </thisDocumentGenerationDateTime>
        </xsl:template>
        <xsl:template xmlns:simpleDateFormat="/java.text.SimpleDateFormat" name="Now">
		
          <xsl:param name="format"/>
		
          <xsl:variable name="nowSimpleDate" select="simpleDateFormat:new($format)"/>
		
          <xsl:variable xmlns:Date="/java.util.Date" name="nowDate" select="Date:new()"></xsl:variable>
		
          <xsl:value-of select="simpleDateFormat:format($nowSimpleDate, $nowDate)"/>
	
        </xsl:template>
