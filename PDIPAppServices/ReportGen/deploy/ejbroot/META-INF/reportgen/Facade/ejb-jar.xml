<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <session>
      <display-name>ReportGen</display-name>
      <ejb-name>ReportGen</ejb-name>
      <home>com.gridnode.pdip.app.reportgen.facade.ejb.IReportGenManagerHome</home>
      <remote>com.gridnode.pdip.app.reportgen.facade.ejb.IReportGenManagerObj</remote>
      <ejb-class>com.gridnode.pdip.app.reportgen.facade.ejb.ReportGenManagerBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>ReportGen</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
