<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
    <enterprise-beans>
        <message-driven>
            <ejb-name>GWFActivityMDBean_BPSS</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFActivityMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			        <activation-config-property>
			        	<activation-config-property-name>messageSelector</activation-config-property-name>
			        	<activation-config-property-value>ENGINE_TYPE = 'BPSS'</activation-config-property-value>
			        </activation-config-property>
			      </activation-config>
        </message-driven>
        <message-driven>
            <ejb-name>GWFActivityMDBean_XPDL</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFActivityMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			        <activation-config-property>
			        	<activation-config-property-name>messageSelector</activation-config-property-name>
			        	<activation-config-property-value>ENGINE_TYPE = 'XPDL'</activation-config-property-value>
			        </activation-config-property>
			      </activation-config>
        </message-driven>
        <message-driven>
            <ejb-name>GWFProcessMDBean_BPSS</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFProcessMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			        <activation-config-property>
			        	<activation-config-property-name>messageSelector</activation-config-property-name>
			        	<activation-config-property-value>ENGINE_TYPE = 'BPSS'</activation-config-property-value>
			        </activation-config-property>
			      </activation-config>
        </message-driven>
        <message-driven>
            <ejb-name>GWFProcessMDBean_XPDL</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFProcessMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			        <activation-config-property>
			        	<activation-config-property-name>messageSelector</activation-config-property-name>
			        	<activation-config-property-value>ENGINE_TYPE = 'XPDL'</activation-config-property-value>
			        </activation-config-property>
			      </activation-config>
        </message-driven>
        <message-driven>
            <ejb-name>GWFRestrictionMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFRestrictionMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
        </message-driven>
        <message-driven>
            <ejb-name>GWFWorklistMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFWorklistMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
        </message-driven>
        <message-driven>
            <ejb-name>GWFCheckStateMDBean_BPSS</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFCheckStateMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			      	<activation-config-property>
			      		<activation-config-property-name>messageSelector</activation-config-property-name>
			      		<activation-config-property-value>Category = 'BPSS'</activation-config-property-value>
			      	</activation-config-property>
			      </activation-config>
       </message-driven>
        <message-driven>
            <ejb-name>GWFCheckStateMDBean_XPDL</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFCheckStateMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
			      <activation-config>
			      	<activation-config-property>
			      		<activation-config-property-name>messageSelector</activation-config-property-name>
			      		<activation-config-property-value>Category = 'XPDL'</activation-config-property-value>
			      	</activation-config-property>
			      </activation-config>
       </message-driven>
        <message-driven>
            <ejb-name>GWFReceiveDocumentMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFReceiveDocumentMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
       </message-driven>
        <message-driven>
            <ejb-name>GWFReceiveSignalMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFReceiveSignalMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
       </message-driven>
        <message-driven>
            <ejb-name>GWFSendSignalMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFSendSignalMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
       </message-driven>
        <message-driven>
            <ejb-name>GWFSendReqDocMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFSendReqDocMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
       </message-driven>
        <message-driven>
            <ejb-name>GWFSendResDocMDBean</ejb-name>
            <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFSendResDocMDBean</ejb-class>
            <transaction-type>Container</transaction-type>
			      <message-destination-type>javax.jms.Queue</message-destination-type>
       </message-driven>
       <message-driven>
				  <ejb-name>GWFInitialiserMDBean</ejb-name>
				  <ejb-class>com.gridnode.pdip.app.workflow.jms.ejb.GWFInitialiserMDBean</ejb-class>
				  <transaction-type>Container</transaction-type>
		      <message-destination-type>javax.jms.Topic</message-destination-type>
					<activation-config>
						<activation-config-property>
							<activation-config-property-name>messageSelector</activation-config-property-name>
							<activation-config-property-value>hostid='${hostid:gnode1}'</activation-config-property-value>
						</activation-config-property>
					</activation-config>      
	   </message-driven>

    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>GWFActivityMDBean_BPSS</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFActivityMDBean_XPDL</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFProcessMDBean_BPSS</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFProcessMDBean_XPDL</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFRestrictionMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFWorklistMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFCheckStateMDBean_BPSS</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFCheckStateMDBean_XPDL</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFReceiveDocumentMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFReceiveSignalMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFSendSignalMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFSendReqDocMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFSendResDocMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <method>
                <ejb-name>GWFInitialiserMDBean</ejb-name>
                <method-name>onMessage</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
			<!--trans-attribute>NotSupported</trans-attribute-->
        </container-transaction>

    </assembly-descriptor>
</ejb-jar>
