<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
  <enterprise-beans>
		<entity>
			<display-name>ProcessDef</display-name>
			<ejb-name>ProcessDef</ejb-name>
			<local-home>com.gridnode.pdip.app.rnif.entities.ejb.IProcessDefLocalHome</local-home>
			<local>com.gridnode.pdip.app.rnif.entities.ejb.IProcessDefLocalObj</local>
			<ejb-class>com.gridnode.pdip.app.rnif.entities.ejb.ProcessDefBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>ProcessDef</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>RequiresNew</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
