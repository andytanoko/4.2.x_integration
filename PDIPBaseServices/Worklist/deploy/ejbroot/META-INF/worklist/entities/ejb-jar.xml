<?xml version="1.0"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
    <enterprise-beans>
        <entity>
            <display-name>WorkListEntity</display-name>
            <ejb-name>WorkListEntity</ejb-name>
            <home>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListEntityHome</home>
            <remote>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListEntityObject</remote>
            <ejb-class>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListEntityBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Long</prim-key-class>
            <reentrant>False</reentrant>
        </entity>
        <entity>
            <display-name>WorkListUserEntity</display-name>
            <ejb-name>WorkListUserEntity</ejb-name>
            <home>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListUserEntityHome</home>
            <remote>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListUserEntityObject</remote>
            <ejb-class>com.gridnode.pdip.base.worklist.entities.ejb.GWFWorkListUserEntityBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.Long</prim-key-class>
            <reentrant>False</reentrant>
        </entity>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>WorkListEntity</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>WorkListUserEntity</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>