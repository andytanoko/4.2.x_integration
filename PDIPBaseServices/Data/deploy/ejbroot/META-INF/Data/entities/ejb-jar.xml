<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
	<enterprise-beans>
		<entity>
			<display-name>BusinessDocument</display-name>
			<ejb-name>BusinessDocument</ejb-name>
			<home>com.gridnode.pdip.base.data.entities.ejb.IBusinessDocumentHome</home>
			<remote>com.gridnode.pdip.base.data.entities.ejb.IBusinessDocumentObject</remote>
			<ejb-class>com.gridnode.pdip.base.data.entities.ejb.BusinessDocumentBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
		<entity>
			<display-name>DataItem</display-name>
			<ejb-name>DataItem</ejb-name>
			<home>com.gridnode.pdip.base.data.entities.ejb.IDataItemHome</home>
			<remote>com.gridnode.pdip.base.data.entities.ejb.IDataItemObject</remote>
			<ejb-class>com.gridnode.pdip.base.data.entities.ejb.DataItemBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
		<entity>
			<display-name>DataItemLocationMap</display-name>
			<ejb-name>DataItemLocationMap</ejb-name>
			<home>com.gridnode.pdip.base.data.entities.ejb.IDataItemLocationMapHome</home>
			<remote>com.gridnode.pdip.base.data.entities.ejb.IDataItemLocationMapObject</remote>
			<ejb-class>com.gridnode.pdip.base.data.entities.ejb.DataItemLocationMapBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
		<entity>
			<display-name>StringData</display-name>
			<ejb-name>StringData</ejb-name>
			<home>com.gridnode.pdip.base.data.entities.ejb.IStringDataHome</home>
			<remote>com.gridnode.pdip.base.data.entities.ejb.IStringDataObject</remote>
			<ejb-class>com.gridnode.pdip.base.data.entities.ejb.StringDataBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
		<entity>
			<display-name>ByteData</display-name>
			<ejb-name>ByteData</ejb-name>
			<home>com.gridnode.pdip.base.data.entities.ejb.IByteDataHome</home>
			<remote>com.gridnode.pdip.base.data.entities.ejb.IByteDataObject</remote>
			<ejb-class>com.gridnode.pdip.base.data.entities.ejb.ByteDataBean</ejb-class>
			<persistence-type>Bean</persistence-type>
			<prim-key-class>java.lang.Long</prim-key-class>
			<reentrant>False</reentrant>
		</entity>
	</enterprise-beans>
	<assembly-descriptor>
		<container-transaction>
			<method>
				<ejb-name>BusinessDocument</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>DataItem</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>DataItemLocationMap</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>StringData</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
		<container-transaction>
			<method>
				<ejb-name>ByteData</ejb-name>
				<method-name>*</method-name>
			</method>
			<trans-attribute>Required</trans-attribute>
		</container-transaction>
	</assembly-descriptor>
</ejb-jar>