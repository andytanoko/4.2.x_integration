<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE server>
 
<!-- Oracle XE-->

<server>
	<mbean code="org.jboss.hibernate.jmx.Hibernate" name="jboss.har:service=ISAT-Hibernate">
        <attribute name="DatasourceName">java:/GtVanDS</attribute>
        <attribute name="Dialect">org.hibernate.dialect.Oracle9Dialect</attribute>
        <attribute name="SessionFactoryName">
            java:/hibernate/AuditTrailISATSessionFactory
        </attribute>

        <attribute name="DefaultSchema">GTVAN</attribute>
        <attribute name="ShowSqlEnabled">true</attribute>
    </mbean>
</server>

