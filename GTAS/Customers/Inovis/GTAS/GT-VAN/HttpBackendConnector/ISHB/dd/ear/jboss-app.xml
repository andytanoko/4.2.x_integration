<?xml version="1.0" encoding="UTF-8"?>
<!--!DOCTYPE jboss-app PUBLIC "-//JBoss//DTD J2EE Application 1.4//EN"
         "http://www.jboss.org/j2ee/dtd/jboss-app_4_0.dtd"-->
<jboss-app>
    <module-order>strict</module-order>
	<loader-repository>
		com.gridnode:loader=gtvan-httpbc-ishb.ear
		<loader-repository-config>
			java2ParentDelegation=true
		</loader-repository-config>
	</loader-repository>
   <module>
      <har>gtvan-httpbc-ishb.har</har>
   </module>
   <module>
		<ejb>gtvan-util-ejb.jar</ejb>
	</module>
   <module>
		<ejb>gtvan-httpbc-ishb-ejb.jar</ejb>
	</module>
    <module>
		<ejb>gtvan-httpbc-hcut-ejb.jar</ejb>
	</module>
	<module>
		<web>
			<web-uri>gtvan-httpbc-ishc.war</web-uri>
			<context-root>/gridtalk/van/httpbc</context-root>
		</web>
	</module>
   <module>
   		<service>ishb-schedule-service.xml</service>
   </module>
</jboss-app> 
