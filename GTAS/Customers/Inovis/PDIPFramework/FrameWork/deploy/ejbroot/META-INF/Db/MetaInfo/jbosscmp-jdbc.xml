<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jbosscmp-jdbc PUBLIC "-//JBoss//DTD JBOSSCMP-JDBC 4.0//EN" "http://www.jboss.org/j2ee/dtd/jbosscmp-jdbc_4_0.dtd">
<jbosscmp-jdbc>
  <defaults>
    <datasource>java:/appdbdatasource</datasource>
    <datasource-mapping>Oracle9i</datasource-mapping>
    <create-table>false</create-table>
    <remove-table>false</remove-table>
    <read-only>false</read-only>
    <row-locking>false</row-locking>
    <pk-constraint>true</pk-constraint>
    <preferred-relation-mapping>foreign-key</preferred-relation-mapping>
    <read-ahead>
      <strategy>on-load</strategy>
       <page-size>500</page-size>
       <eager-load-group>*</eager-load-group>
    </read-ahead>
  </defaults>
  <enterprise-beans>
    <entity>
      <ejb-name>FieldMetaInfo</ejb-name>
      <datasource>java:/appdbdatasource</datasource>
      <datasource-mapping>Oracle9i</datasource-mapping>
      <create-table>false</create-table>
      <remove-table>false</remove-table>
      <read-only>false</read-only>
      <row-locking>false</row-locking>
      <pk-constraint>true</pk-constraint>
      <read-ahead>
         <strategy>on-load</strategy>
         <page-size>500</page-size>
         <eager-load-group>*</eager-load-group>
      </read-ahead>
      <table-name>"fieldmetainfo"</table-name>
      <cmp-field>
        <field-name>uId</field-name>
        <column-name>"UID"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>displayable</field-name>
        <column-name>"Displayable"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>editable</field-name>
        <column-name>"Editable"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>mandatory</field-name>
        <column-name>"Mandatory"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>proxy</field-name>
        <column-name>"Proxy"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>label</field-name>
        <column-name>"Label"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>length</field-name>
        <column-name>"Length"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>sequence</field-name>
        <column-name>"Sequence"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>fieldName</field-name>
        <column-name>"FieldName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>valueClass</field-name>
        <column-name>"ValueClass"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>sqlName</field-name>
        <column-name>"SqlName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>oqlName</field-name>
        <column-name>"OqlName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>objectName</field-name>
        <column-name>"ObjectName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>entityName</field-name>
        <column-name>"EntityObjectName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>presentation</field-name>
        <column-name>"Presentation"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>constraints</field-name>
        <column-name>"Constraints"</column-name>
      </cmp-field>
    </entity>
    <entity>
      <ejb-name>EntityMetaInfo</ejb-name>
      <datasource>java:/appdbdatasource</datasource>
      <datasource-mapping>Oracle9i</datasource-mapping>
      <create-table>false</create-table>
      <remove-table>false</remove-table>
      <read-only>false</read-only>
      <row-locking>false</row-locking>
      <pk-constraint>true</pk-constraint>
      <read-ahead>
        <strategy>on-load</strategy>
         <page-size>500</page-size>
         <eager-load-group>*</eager-load-group>
      </read-ahead>
      <table-name>"entitymetainfo"</table-name>
      <cmp-field>
        <field-name>objectName</field-name>
        <column-name>"ObjectName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>entityName</field-name>
        <column-name>"EntityName"</column-name>
      </cmp-field>
      <cmp-field>
        <field-name>sqlName</field-name>
        <column-name>"SqlName"</column-name>
      </cmp-field>
    </entity>
  </enterprise-beans>
</jbosscmp-jdbc>
