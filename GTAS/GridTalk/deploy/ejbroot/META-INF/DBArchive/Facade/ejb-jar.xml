<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
  <session>
      <display-name>ArchiveManager</display-name>
      <ejb-name>ArchiveManager</ejb-name>
      <home>com.gridnode.gtas.server.dbarchive.facade.ejb.IArchiveManagerHome</home>
      <remote>com.gridnode.gtas.server.dbarchive.facade.ejb.IArchiveManagerObj</remote>
      <ejb-class>com.gridnode.gtas.server.dbarchive.facade.ejb.ArchiveManagerBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
    <session>
      <display-name>ProcessDocumentManager</display-name>
      <ejb-name>ProcessDocumentManager</ejb-name>
      <home>com.gridnode.gtas.server.dbarchive.facade.ejb.IProcessDocumentManagerHome</home>
      <remote>com.gridnode.gtas.server.dbarchive.facade.ejb.IProcessDocumentManagerObj</remote>
      <ejb-class>com.gridnode.gtas.server.dbarchive.facade.ejb.ProcessDocumentManagerBean</ejb-class>
      <session-type>Stateful</session-type>
      <transaction-type>Container</transaction-type>
    </session>
	<session>
      <display-name>SearchESDocumentManager</display-name>
      <ejb-name>SearchESDocumentManager</ejb-name>
      <home>com.gridnode.gtas.server.dbarchive.facade.ejb.ISearchESDocumentManagerHome</home>
      <remote>com.gridnode.gtas.server.dbarchive.facade.ejb.ISearchESDocumentManagerObj</remote>
      <ejb-class>com.gridnode.gtas.server.dbarchive.facade.ejb.SearchESDocumentManagerBean</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
	  <method>
              <ejb-name>ArchiveManager</ejb-name>
              <method-name>*</method-name>
      </method>
      <method>
              <ejb-name>ProcessDocumentManager</ejb-name>
              <method-name>*</method-name>
      </method>
	  <method>
              <ejb-name>SearchESDocumentManager</ejb-name>
              <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
