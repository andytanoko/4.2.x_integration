<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
  <enterprise-beans>
    <entity>
      <display-name>ResponseTrackRecord</display-name>
      <ejb-name>ResponseTrackRecord</ejb-name>
      <local-home>com.gridnode.gtas.server.docalert.entities.ejb.IResponseTrackRecordLocalHome</local-home>
      <local>com.gridnode.gtas.server.docalert.entities.ejb.IResponseTrackRecordLocalObj</local>
      <ejb-class>com.gridnode.gtas.server.docalert.entities.ejb.ResponseTrackRecordBean</ejb-class>
      <persistence-type>Bean</persistence-type>
      <prim-key-class>java.lang.Long</prim-key-class>
      <reentrant>False</reentrant>
    </entity>
    <entity>
      <display-name>ReminderAlert</display-name>
      <ejb-name>ReminderAlert</ejb-name>
      <local-home>com.gridnode.gtas.server.docalert.entities.ejb.IReminderAlertLocalHome</local-home>
      <local>com.gridnode.gtas.server.docalert.entities.ejb.IReminderAlertLocalObj</local>
      <ejb-class>com.gridnode.gtas.server.docalert.entities.ejb.ReminderAlertBean</ejb-class>
      <persistence-type>Bean</persistence-type>
      <prim-key-class>java.lang.Long</prim-key-class>
      <reentrant>False</reentrant>
    </entity>
    <entity>
      <display-name>ActiveTrackRecord</display-name>
      <ejb-name>ActiveTrackRecord</ejb-name>
      <local-home>com.gridnode.gtas.server.docalert.entities.ejb.IActiveTrackRecordLocalHome</local-home>
      <local>com.gridnode.gtas.server.docalert.entities.ejb.IActiveTrackRecordLocalObj</local>
      <ejb-class>com.gridnode.gtas.server.docalert.entities.ejb.ActiveTrackRecordBean</ejb-class>
      <persistence-type>Bean</persistence-type>
      <prim-key-class>java.lang.Long</prim-key-class>
      <reentrant>False</reentrant>
    </entity>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
              <ejb-name>ResponseTrackRecord</ejb-name>
              <method-name>*</method-name>
      </method>
      <method>
              <ejb-name>ReminderAlert</ejb-name>
              <method-name>*</method-name>
      </method>
      <method>
              <ejb-name>ActiveTrackRecord</ejb-name>
              <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
  </assembly-descriptor>
</ejb-jar>
