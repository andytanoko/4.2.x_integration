<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-j2ee-engine SYSTEM "ejb-j2ee-engine.dtd">
<ejb-j2ee-engine>
	<enterprise-beans>
		<enterprise-bean>
			<ejb-name>DocAlertManager</ejb-name>
			<jndi-name>com.gridnode.gtas.server.docalert.facade.ejb.IDocAlertManagerHome</jndi-name>
			<session-props/>
		</enterprise-bean>
		<enterprise-bean>
			<ejb-name>ReminderAlertMDBean</ejb-name>
			<message-props>
				<destination-name>timeInvokeQueue</destination-name>
                                <connection-factory-name>GNQueueConnectionFactory</connection-factory-name>
			</message-props>
		</enterprise-bean>
		<enterprise-bean>
			<ejb-name>DocumentActivityMDBean</ejb-name>
			<message-props>
				<destination-name>notifierTopic</destination-name>
                                <connection-factory-name>GNTopicConnectionFactory</connection-factory-name>
			</message-props>
		</enterprise-bean>
		<enterprise-bean>
			<ejb-name>DocAlertRequestMDBean</ejb-name>
			<message-props>
				<destination-name>notifierTopic</destination-name>
                                <connection-factory-name>GNTopicConnectionFactory</connection-factory-name>
			</message-props>
		</enterprise-bean>
	</enterprise-beans>
</ejb-j2ee-engine>