<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/j2ee" version="2.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
                             http://java.sun.com/xml/ns/j2ee/ejb-jar_2_1.xsd">
        <enterprise-beans>
                <entity>
                        <display-name>DocumentType</display-name>
                        <ejb-name>DocumentType</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IDocumentTypeLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IDocumentTypeLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.DocumentTypeBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
                <entity>
                        <display-name>FileType</display-name>
                        <ejb-name>FileType</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IFileTypeLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IFileTypeLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.FileTypeBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
                <entity>
                        <display-name>GridDocument</display-name>
                        <ejb-name>GridDocument</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IGridDocumentLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IGridDocumentLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.GridDocumentBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
                <entity>
                        <display-name>Attachment</display-name>
                        <ejb-name>Attachment</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IAttachmentLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IAttachmentLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.AttachmentBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
                <entity>
                        <display-name>AttachmentRegistry</display-name>
                        <ejb-name>AttachmentRegistry</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IAttachmentRegistryLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IAttachmentRegistryLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.AttachmentRegistryBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
				 <entity>
                        <display-name>AS2DocTypeMapping</display-name>
                        <ejb-name>AS2DocTypeMapping</ejb-name>
                        <local-home>com.gridnode.gtas.server.document.entities.ejb.IAS2DocTypeMappingLocalHome</local-home>
                        <local>com.gridnode.gtas.server.document.entities.ejb.IAS2DocTypeMappingLocalObj</local>
                        <ejb-class>com.gridnode.gtas.server.document.entities.ejb.AS2DocTypeMappingBean</ejb-class>
                        <persistence-type>Bean</persistence-type>
                        <prim-key-class>java.lang.Long</prim-key-class>
                        <reentrant>False</reentrant>
                </entity>
        </enterprise-beans>
        <assembly-descriptor>
                <container-transaction>
                        <method>
                                <ejb-name>DocumentType</ejb-name>
                                <method-name>*</method-name>
                        </method>
                        <method>
                                <ejb-name>FileType</ejb-name>
                                <method-name>*</method-name>
                        </method>
                        <method>
                                <ejb-name>GridDocument</ejb-name>
                                <method-name>*</method-name>
                        </method>
                        <method>
                                <ejb-name>Attachment</ejb-name>
                                <method-name>*</method-name>
                        </method>
                        <method>
                                <ejb-name>AttachmentRegistry</ejb-name>
                                <method-name>*</method-name>
                        </method>
						<method>
                                <ejb-name>AS2DocTypeMapping</ejb-name>
                                <method-name>*</method-name>
                        </method>
                        <trans-attribute>Required</trans-attribute>
                </container-transaction>
        </assembly-descriptor>
</ejb-jar>
